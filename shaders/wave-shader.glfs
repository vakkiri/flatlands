#version 420
uniform sampler2D texUnit;
uniform float time;

in vec2 texCoord;
layout(location = 0) out vec4 gl_FragColor;

void main() {
	float dy = sin((time * 2) + (3.14 / 5.f * cos(texCoord.x * 768.f / 4.f))) * (64.f / 512.f);
	vec2 coord = texCoord;
	coord.y += dy;
	gl_FragColor = texture( texUnit, coord );
}

